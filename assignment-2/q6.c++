// calculator to operate on + - * / %
#include<iostream>
using namespace std;
int main(){
    float a,b;

    cout<<"a : ";
    cin>>a;

    cout<<"b : ";
    cin>>b;

    int sum,diff,product,division;
    cout<<"sum = "<<a+b<<endl;
    cout<<"difference = "<<a-b<<endl;
    cout<<"product = "<<a*b<<endl;
    cout<<"division= "<<a/b<<endl;

    return 0;


}
